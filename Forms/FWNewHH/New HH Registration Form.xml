      <bind nodeset="/data/existing_location" type="string"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/i_start_time" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/i_end_time" type="dateTime"/>
      <bind constraint=".&lt;= /data/today " jr:constraintMsg="jr:itext('/data/FWNHREGDATE:jr:constraintMsg')" nodeset="/data/FWNHREGDATE" required="true()" type="date"/>
      <bind constraint="regex(., '^[0-9]{4}$')" jr:constraintMsg="jr:itext('/data/FWGOBHHID:jr:constraintMsg')" nodeset="/data/FWGOBHHID" required="true()" type="string"/>
      <bind constraint="regex(., '^[0-9]{4}$')" jr:constraintMsg="jr:itext('/data/FWJIVHHID:jr:constraintMsg')" nodeset="/data/FWJIVHHID" required="true()" type="string"/>
      <bind nodeset="/data/FWNHNEARTO" type="string"/>
      <bind nodeset="/data/FWNHHHGPS" required="true()" type="geopoint"/>
      <bind nodeset="/data/FWHOHFNAME" required="true()" type="string"/>
      <bind calculate="&quot;.&quot;" nodeset="/data/FWHOHLNAME" type="string"/>
      <bind calculate="&quot;1900-01-01&quot;" nodeset="/data/FWHOHBIRTHDATE" type="string"/>
      <bind calculate="&quot;male&quot;" nodeset="/data/FWHOHGENDER" type="string"/>
      <bind nodeset="/data/FWNHHMBRNUM" type="int"/>
      <bind nodeset="/data/FWNHHMWRA" type="select1"/>
      <bind nodeset="/data/woman" relevant=" /data/FWNHHMWRA ='1'"/>
      <bind calculate=" /data/FWGOBHHID " nodeset="/data/woman/GOBHHID" type="string"/>
      <bind calculate=" /data/FWJIVHHID " nodeset="/data/woman/JiVitAHHID" type="string"/>
      <bind nodeset="/data/woman/FWWOMFNAME" required="true()" type="string"/>
      <bind calculate="&quot;.&quot;" nodeset="/data/woman/FWWOMLNAME" type="string"/>
      <bind constraint="regex(., '^[0-9]{13}$') or regex(., '^[0-9]{17}$')" jr:constraintMsg="jr:itext('/data/woman/FWWOMNID:jr:constraintMsg')" nodeset="/data/woman/FWWOMNID" type="string"/>
      <bind constraint="regex(., '^[0-9]{17}$')" jr:constraintMsg="jr:itext('/data/woman/FWWOMBID:jr:constraintMsg')" nodeset="/data/woman/FWWOMBID" required="true()" type="string"/>
      <bind nodeset="/data/woman/FWHUSNAME" required="true()" type="string"/>
      <bind constraint="(( /data/woman/FWBIRTHDATE )+4745) &lt;= today()" jr:constraintMsg="jr:itext('/data/woman/FWBIRTHDATE:jr:constraintMsg')" nodeset="/data/woman/FWBIRTHDATE" required="true()" type="date"/>
      <bind calculate="&quot;female&quot;" nodeset="/data/woman/FWGENDER" type="string"/>
      <bind calculate="round((( /data/today  -  /data/woman/FWBIRTHDATE ) div 365.25),0)" nodeset="/data/woman/FWWOMAGE" type="string"/>
      <bind nodeset="/data/woman/display_age" readonly="true()" type="string"/>
      <bind nodeset="/data/woman/FWNHWOMSTRMEN" required="true()" type="select1"/>
      <bind nodeset="/data/woman/FWNHWOMHUSALV" relevant=" /data/woman/FWNHWOMSTRMEN ='0'" required="true()" type="select1"/>
      <bind nodeset="/data/woman/FWNHWOMHUSLIV" relevant=" /data/woman/FWNHWOMSTRMEN ='0' and  /data/woman/FWNHWOMHUSALV ='1'" required="true()" type="select1"/>
      <bind nodeset="/data/woman/FWNHWOMHUSSTR" relevant=" /data/woman/FWNHWOMSTRMEN ='0' and  /data/woman/FWNHWOMHUSLIV ='1'" required="true()" type="select1"/>
      <bind calculate="if( /data/woman/FWNHWOMSTRMEN ='0' and  /data/woman/FWNHWOMHUSLIV ='1' and  /data/woman/FWNHWOMHUSSTR ='0',1,0)" nodeset="/data/woman/FWELIGIBLE" type="string"/>
      <bind nodeset="/data/woman/add_women" readonly="true()" type="string"/>
      <bind calculate="join(' ',  /data/woman/FWWOMFNAME )" nodeset="/data/join_names" type="string"/>
      <bind calculate="if( /data/join_names  = '', 0, count( /data/woman ))" nodeset="/data/MWRA" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
